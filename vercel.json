{
  "version": 2,
  "name": "udcar-rent",
  "builds": [
    { "src": "package.json", "use": "@vercel/next" }
  ],
  "cleanUrls": true,
  "headers": [
    {
      "source": "\\/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=0, s-maxage=600" }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/www/:path*",
      "destination": "/:path*",
      "permanent": true
    }
  ]
}
